<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.fom.proyecto1">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_app_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="org.fom.proyecto1.MainActivity"
            android:theme="@style/AppTheme">
        </activity>
        <!--actividades con sin tool bar por defecto-->
        <activity android:name="org.fom.proyecto1.RegistroActivity"
            android:theme="@style/temasinactionbar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegistroDatosPersonales"
            android:label="@string/datos_personales"/>
        <activity android:name=".EnvioRespuestaPersonal"
            android:label="Demanda de servicio"/>
        <activity android:name=".PagoServicio"
            android:label="@string/realizacion_pago"/>

        <activity android:name="org.fom.proyecto1.PrincipalActivity"
            android:theme="@style/temasinactionbar">
        </activity>
        <activity android:name="org.fom.proyecto1.LoginActivity"
            android:theme="@style/temasinactionbar">
        </activity>
        <!--otras-->
    </application>
</manifest>